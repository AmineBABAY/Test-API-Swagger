<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AR-T168 Swagger API : Make calls to the Endpoint /documents/{documentId}/printable">

	<test name="Make requests to the endpoint printable">
		<classes>
			<class name="com.arender.tests.PrintableTest">
				<methods>
					<include name="getPrintableDocumentOfPdf" />
					<include name="getPrintableDocumentOfJpg" />
					<include name="getPrintableDocumentOfTiff" />
					<include name="getPrintableDocumentOfDoc" />
					<include name="getPrintableDocumentOfZip" />
				</methods>
			</class>
		</classes>
	</test>
</suite>